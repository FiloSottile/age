    "version": "0.2.0",
    "configurations": [
        {
            "name": "Encrypt",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/age",
            "args": [
                "-e",
                "-r",
                "agePQ.",
                "-o",
                "${workspaceFolder}/debug/debug-out",
                "${workspaceFolder}/AUTHORS",
            ],
        },
        {
            "name": "Decrypt",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/age",
            "args": [
                "-d",
                "-i",
                "${workspaceFolder}/debug/agepq.key",
                "${workspaceFolder}/debug/debug-out"
            ],
        }
    ]
}